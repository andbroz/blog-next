    "gatsby": "^3.10.1",
    "prismjs": "^1.24.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.2.4",
    "tailwindcss": "^2.2.6",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.1",
