{
  "name": "react-blog",
  "private": true,
  "description": "A blog system based on React and Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "postbuild": "rm -fr ./build && mv ./public ./build",
    "build:gh-pages": "gatsby build --prefix-paths && npm run postbuild",
    "develop": "gatsby develop --open",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "antd": "^3.15.2",
    "gatsby": "^2.5.7",
    "gatsby-plugin-antd": "^2.0.2",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-manifest": "^2.0.22",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.8",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.9",
    "gatsby-remark-prismjs": "^3.2.4",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-remark": "^2.3.0",
    "prismjs": "^1.15.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-spring": "^8.0.11",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.85.0",
    "typed.js": "^2.0.10"
  },
  "devDependencies": {
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sabertazimi/blog"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/blog/issues"
  }
}
